<form method="post" action="{{ path('delete_supply', {'id': supply.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ supply.id) }}">
    <button class="btn btn-outline-dark btn-sm">
        <i class="fa-regular fa-trash-can"></i>
    </button>
</form>
