{% extends 'base.html.twig' %}

{% block title %}ProductSerialized{% endblock %}

{% block body %}
    <h1>ProductSerialized</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ product_serialized.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ product_serialized.name }}</td>
            </tr>
            <tr>
                <th>Reference</th>
                <td>{{ product_serialized.reference }}</td>
            </tr>
            <tr>
                <th>Serial_number</th>
                <td>{{ product_serialized.serialNumber }}</td>
            </tr>
            <tr>
                <th>Quantity</th>
                <td>{{ product_serialized.quantity }}</td>
            </tr>
            <tr>
                <th>Brand</th>
                <td>{{ product_serialized.brand }}</td>
            </tr>
            <tr>
                <th>AddedAt</th>
                <td>{{ product_serialized.addedAt ? product_serialized.addedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_product_serialized_index') }}">back to list</a>

    <a href="{{ path('app_product_serialized_edit', {'id': product_serialized.id}) }}">edit</a>

    {{ include('product_serialized/_delete_form.html.twig') }}
{% endblock %}
