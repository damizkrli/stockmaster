{% extends 'base.html.twig' %}

{% block title %}OutOfStock{% endblock %}

{% block body %}
    <h1>OutOfStock</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ out_of_stock.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ out_of_stock.name }}</td>
            </tr>
            <tr>
                <th>Reference</th>
                <td>{{ out_of_stock.reference }}</td>
            </tr>
            <tr>
                <th>Quantity</th>
                <td>{{ out_of_stock.quantity }}</td>
            </tr>
            <tr>
                <th>AddedAt</th>
                <td>{{ out_of_stock.addedAt ? out_of_stock.addedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Brand</th>
                <td>{{ out_of_stock.brand }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_out_of_stock_index') }}">back to list</a>

    <a href="{{ path('app_out_of_stock_edit', {'id': out_of_stock.id}) }}">edit</a>

    {{ include('out_of_stock/_delete_form.html.twig') }}
{% endblock %}
